# Oura API (create app at https://cloud.ouraring.com/oauth/applications)
OURA_CLIENT_ID=your_client_id
OURA_CLIENT_SECRET=your_client_secret
# Must match EXACTLY the redirect URI in your Oura app (Oura will send the user here with ?code=...)
OURA_REDIRECT_URI=http://localhost:3001/api/oura/callback
# Where to send the user after OAuth (your app)
OURA_FRONTEND_URL=http://localhost:5173
# Optional: Oura server port (default 3001)
OURA_SERVER_PORT=3001
